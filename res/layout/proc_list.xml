<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:layout_width="fill_parent"
	android:layout_height="fill_parent" xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:duplicateParentState="false"
	android:background="#EEEEEE">

	<ListView android:layout_width="fill_parent"
		android:layout_height="0dip" android:cacheColorHint="#00000000"
		android:divider="@null" android:paddingLeft="5dip"
		android:paddingRight="5dip" android:id="@+id/listproc"
		android:layout_weight="1"></ListView>
	<LinearLayout android:background="#101010"
		android:layout_width="fill_parent" android:layout_height="wrap_content"
		android:orientation="horizontal" android:id="@+id/proc_menu_linear">
		<LinearLayout android:id="@+id/close_select_process_linear"
			android:layout_weight="1" android:background="@layout/linear_bg_selector"
			android:layout_width="fill_parent" android:layout_height="wrap_content"
			android:paddingTop="3dip" android:paddingBottom="3dip"
			android:orientation="vertical" android:gravity="center">
			<TextView android:id="@+id/mobile_exp_icon_tv"
				android:background="@drawable/close_select_proess_bg"
				android:layout_width="wrap_content" android:layout_height="wrap_content" />
			<TextView android:id="@+id/mobile_exp_tv" android:text="@string/proc_close_select"
				android:textColor="@color/light_green" android:layout_width="wrap_content"

				android:layout_height="wrap_content" />
		</LinearLayout>
		<LinearLayout android:id="@+id/close_al_process_linear"
			android:background="@layout/linear_bg_selector" android:gravity="center"
			android:layout_weight="1" android:layout_width="fill_parent"
			android:paddingTop="3dip" android:paddingBottom="3dip"
			android:layout_height="wrap_content" android:orientation="vertical">
			<TextView android:id="@+id/use_fuc_icon_tv"
				android:background="@drawable/close_proess_bg" android:layout_width="wrap_content"
				android:layout_height="wrap_content" />
			<TextView android:id="@+id/use_fuc_tv" android:text="@string/proc_close_al"
				android:textColor="@color/light_green" android:layout_width="wrap_content"
				android:layout_height="wrap_content" />
		</LinearLayout>

	</LinearLayout>

</LinearLayout>